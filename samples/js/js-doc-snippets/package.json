{
  "name": "js-doc-snippets",
  "version": "0.0.1",
  "type": "module",
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "dependencies": {
    "@kalix-io/kalix-javascript-sdk": "1.0.0"
  },
  "devDependencies": {
    "@kalix-io/kalix-scripts": "1.0.0",
    "@kalix-io/testkit": "1.0.0",
    "chai": "^4.3.6",
    "jsdoc": "^3.6.10",
    "mocha": "^10.0.0",
    "uglify-js": "^3.16.1"
  },
  "config": {
    "dockerImage": "gcr.io/kalix-public/samples-js-doc-snippets",
    "sourceDir": "./src",
    "testSourceDir": "./test",
    "protoSourceDir": "./proto",
    "generatedSourceDir": "./lib/generated",
    "compileDescriptorArgs": []
  },
  "scripts": {
    "start": "node src/index.js",
    "test": "mocha ./test",
    "integration-test": "mocha ./integration-test",
    "test-all": "npm run test && npm run integration-test",
    "build": "kalix-scripts build",
    "package": "kalix-scripts package",
    "deploy": "kalix-scripts deploy"
  }
}
