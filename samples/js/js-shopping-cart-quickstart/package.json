{
  "name": "shopping-cart",
  "version": "0.0.1",
  "type": "module",
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "dependencies": {
    "@kalix-io/kalix-javascript-sdk": "^1.0.2"
  },
  "devDependencies": {
    "@kalix-io/kalix-scripts": "^1.0.2",
    "@kalix-io/testkit": "^1.0.2",
    "chai": "^4.3.6",
    "mocha": "^10.0.0"
  },
  "config": {
    "dockerImage": "my-docker-repo/shopping-cart",
