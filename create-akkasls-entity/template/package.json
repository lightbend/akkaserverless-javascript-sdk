{
  "name": "{{name}}",
  "version": "0.0.1",
  "type": "module",
  "engines": {
    "node": ">=13.0.0",
    "npm": ">=6.0.0"
  },
  "dependencies": {
    "@grpc/proto-loader": "^0.1.0",
    "@lightbend/akkaserverless-javascript-sdk": "0.7.0-beta.1",
    "google-protobuf": "^3.0.0",
    "grpc": "^1.20.2"
  },
  "devDependencies": {
    "@lightbend/akkasls-scripts": "^{{libraryVersion}}",
    "chai": "^4.3.3",
    "mocha": "^8.3.1"
  },
  "config": {
    "dockerImage": "my-docker-repo/{{name}}",
    "sourceDir": "./src",
    "testSourceDir": "./test",
    "protoSourceDir": "./proto",
    "generatedSourceDir": "./lib/generated"
  },
  "scripts": {
    "start": "node src/index.js",
    "test": "mocha ./test",
    "build": "akkasls-scripts build",
    "package": "akkasls-scripts package",
    "deploy": "akkasls-scripts deploy"
  }
}
